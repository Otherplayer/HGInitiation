# Uncomment this line to define a global platform for your project
platform :ios, '8.2'

#Use frameworks instead of static libraries for Pods.
use_frameworks!

# ignore all warnings from all pods
inhibit_all_warnings!

target "HGInitiation" do

    pod 'AFNetworking'
    pod 'SDWebImage'
    pod 'MBProgressHUD'
    pod 'MJRefresh'
    pod 'YYKit'
    pod 'PinYin4Objc'
    pod 'CocoaSecurity'

end
